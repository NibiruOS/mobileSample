<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="nibiru_mobile_sample">
	<inherits name="ar.com.oxen.nibiru.mobile.mgwt.Module" />
	<inherits name="ar.com.oxen.nibiru.mobile.security.mgwt.Module" />
	<inherits name="ar.com.oxen.nibiru.mobile.sample.app.Module" />
	<set-configuration-property name='xsiframe.failIfScriptTag'
		value='FALSE' />
	<set-property name="user.agent" value="safari" />
	<source path="app" />
	<source path="data" />
	<source path="ui" />
	<entry-point
		class="ar.com.oxen.nibiru.mobile.sample.mgwt.app.GwtEntryPoint" />
</module>