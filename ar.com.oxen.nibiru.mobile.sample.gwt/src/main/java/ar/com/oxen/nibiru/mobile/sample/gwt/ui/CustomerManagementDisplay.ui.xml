<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.googlecode.mgwt.ui.client.widget">

	<ui:with field='msg'
		type='ar.com.oxen.nibiru.mobile.sample.gwt.ui.GwtSampleMessages' />
	<ui:with field='res'
		type='ar.com.oxen.nibiru.mobile.sample.gwt.ui.Resources' />

	<ui:style>
		.title {
			left: 10px;
			position: absolute;
			top: 15px;
		}
		.add {
			position: absolute;
			right: 10px;
			top: 10px;
		}
		.list {
			bottom: 10px;
			left: 10px;
			position: absolute;
			right: 10px;
			top: 50px;
		}
	</ui:style>
	<g:HTMLPanel width="100%">
		<g:Label text="{msg.customerManagement}" styleName="{res.css.title} {style.title}" />
		<g:FlowPanel styleName="{style.add}" >
			<m:button.ImageButton ui:field="add" icon='{res.add}' />
		</g:FlowPanel>
		<g:FlowPanel styleName="{style.list}" >
			<m:list.celllist.CellList ui:field="list" width="100%" height="100%" />
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 